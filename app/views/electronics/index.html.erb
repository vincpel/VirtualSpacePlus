<p id="notice"><%= notice %></p>

<h1>Listing Electronics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @electronics.each do |electronic| %>
      <tr>
        <td><%= link_to 'Show', electronic %></td>
        <td><%= link_to 'Edit', edit_electronic_path(electronic) %></td>
        <td><%= link_to 'Destroy', electronic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Electronic', new_electronic_path %>
