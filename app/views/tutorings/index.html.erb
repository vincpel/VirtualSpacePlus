<p id="notice"><%= notice %></p>

<h1>Listing Tutorings</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutorings.each do |tutoring| %>
      <tr>
        <td><%= tutoring.course %></td>
        <td><%= link_to 'Show', tutoring %></td>
        <td><%= link_to 'Edit', edit_tutoring_path(tutoring) %></td>
        <td><%= link_to 'Destroy', tutoring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutoring', new_tutoring_path %>
